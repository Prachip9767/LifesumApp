<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UI.fragments.DiaryFragment">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:ignore="UselessParent">

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize"
            android:background="@drawable/ctr_bg"
            app:layout_collapseMode="pin"
            app:titleTextColor="@color/black">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginEnd="15dp"
                android:background="@drawable/ctr_bg"
                android:text="@string/classic_dieting"
                android:textAlignment="center"
                android:textColor="@color/white"
                android:textSize="25sp" />
        </androidx.appcompat.widget.Toolbar>

        <androidx.coordinatorlayout.widget.CoordinatorLayout
            android:id="@+id/cocordinate"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <com.google.android.material.appbar.AppBarLayout
                android:id="@+id/app_bar"
                android:layout_width="match_parent"
                android:layout_height="306dp"
                android:background="@drawable/ctr_bg"
                android:fitsSystemWindows="true">

                <com.google.android.material.appbar.CollapsingToolbarLayout
                    android:id="@+id/toolbar_layout"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/ctr_bg"
                    android:fitsSystemWindows="true"
                    app:contentScrim="?attr/colorPrimary"
                    app:layout_scrollFlags="scroll|exitUntilCollapsed|snap"
                    app:toolbarId="@+id/collapsingToolbar">

                    <com.airbnb.lottie.LottieAnimationView
                        android:id="@+id/animationView1"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_gravity="center|bottom"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/app_bar"
                        app:lottie_autoPlay="true"
                        app:lottie_loop="true"
                        app:lottie_rawRes="@raw/arrow" />


                    <com.mikhaellopez.circularprogressbar.CircularProgressBar
                        android:id="@+id/circularProgressBar"
                        android:layout_width="148dp"
                        android:layout_height="149dp"
                        android:layout_gravity="center|top"
                        android:layout_marginTop="50dp"
                        android:textAllCaps="true"
                        app:cpb_background_progressbar_color="#88d8c0"
                        app:cpb_background_progressbar_width="5dp"
                        app:cpb_progress_direction="to_right"
                        app:cpb_progressbar_color="#f2f3f4"
                        app:cpb_progressbar_width="10dp"
                        app:cpb_round_border="false" />

                    <TextView
                        android:id="@+id/tv_eaten"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="50dp"
                        android:layout_marginTop="110dp"
                        android:scaleType="centerCrop"
                        android:text="0"
                        android:textColor="@color/white"
                        android:textSize="20sp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center|start"
                        android:layout_marginStart="30dp"
                        android:scaleType="centerCrop"
                        android:text="@string/eaten"
                        android:textAllCaps="true"
                        android:textColor="@color/white"
                        android:textSize="20sp" />

                    <TextView
                        android:id="@+id/tv_kacl_left"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="170dp"
                        android:layout_marginTop="90dp"
                        android:scaleType="centerCrop"
                        android:text="1486"
                        android:textColor="@color/white"
                        android:textSize="20sp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:scaleType="centerCrop"
                        android:text="@string/kcal_left"
                        android:textColor="@color/white"
                        android:textSize="20sp" />

                    <TextView
                        android:id="@+id/tv_burned"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="end"
                        android:layout_marginTop="110dp"
                        android:layout_marginEnd="60dp"
                        android:scaleType="centerCrop"
                        android:text="0"
                        android:textColor="@color/white"
                        android:textSize="20sp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center|end"
                        android:layout_marginEnd="30dp"
                        android:scaleType="centerCrop"
                        android:text="@string/burned"
                        android:textAllCaps="true"
                        android:textColor="@color/white"
                        android:textSize="20sp" />

                    <TextView
                        android:id="@+id/tv_protein"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center|bottom"
                        android:layout_marginBottom="40dp"
                        android:text="74g left"
                        android:textColor="@color/white"
                        android:textSize="15sp"
                        app:layout_collapseMode="parallax" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginTop="70dp"
                        android:scaleType="centerCrop"
                        android:text="@string/protein"
                        android:textAllCaps="true"
                        android:textColor="@color/white"
                        android:textSize="20sp"
                        app:layout_collapseMode="parallax" />

                    <TextView
                        android:id="@+id/tv_carbs"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center|bottom|start"
                        android:layout_marginStart="30dp"
                        android:layout_marginBottom="40dp"
                        android:text="186g left"
                        android:textColor="@color/white"
                        android:textSize="15sp"
                        app:layout_collapseMode="parallax" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center|start"
                        android:layout_marginStart="30dp"
                        android:layout_marginTop="70dp"
                        android:scaleType="centerCrop"
                        android:text="@string/carbs"
                        android:textAllCaps="true"
                        android:textColor="@color/white"
                        android:textSize="20sp"
                        app:layout_collapseMode="parallax" />

                    <TextView
                        android:id="@+id/tv_fat"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center|bottom|end"
                        android:layout_marginEnd="30dp"
                        android:layout_marginBottom="40dp"
                        android:text="50g left"
                        android:textColor="@color/white"
                        android:textSize="15sp"
                        app:layout_collapseMode="parallax" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center|end"
                        android:layout_marginTop="70dp"
                        android:layout_marginEnd="45dp"
                        android:scaleType="centerCrop"
                        android:text="@string/fat"
                        android:textColor="@color/white"
                        android:textSize="20sp"
                        app:layout_collapseMode="parallax" />
                </com.google.android.material.appbar.CollapsingToolbarLayout>

            </com.google.android.material.appbar.AppBarLayout>

            <androidx.core.widget.NestedScrollView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/layout_bg"
                app:layout_behavior="com.google.android.material.appbar.AppBarLayout$ScrollingViewBehavior">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">


                    <RelativeLayout
                        android:id="@+id/Relative"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/select_date"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:padding="12dp"
                            android:text="select date"
                            android:textColor="#242424"
                            android:textSize="22sp"
                            android:textStyle="bold" />
                    </RelativeLayout>
                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical">


                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:padding="10dp"
                                    android:text="Water"
                                    android:textSize="30sp" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentEnd="true"
                                    android:padding="10dp"
                                    android:text="0 L"
                                    android:textSize="30sp" />

                            </RelativeLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">


                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:id="@+id/layout1"
                                    android:layout_margin="2dp"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content">

                                    <com.airbnb.lottie.LottieAnimationView
                                        android:id="@+id/glassA"
                                        android:layout_width="50dp"
                                        android:layout_height="50dp"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toTopOf="parent"
                                        app:lottie_autoPlay="false"
                                        app:lottie_loop="false"
                                        app:lottie_rawRes="@raw/glass_fill" />

                                    <com.airbnb.lottie.LottieAnimationView
                                        android:id="@+id/glassAa"
                                        android:layout_width="50dp"
                                        android:layout_height="50dp"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toTopOf="parent"
                                        app:lottie_autoPlay="false"
                                        app:lottie_loop="false"
                                        app:lottie_rawRes="@raw/glass_fill_reverse" />
                                </androidx.constraintlayout.widget.ConstraintLayout>

                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:id="@+id/layout2"
                                    android:layout_margin="2dp"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content">

                                    <com.airbnb.lottie.LottieAnimationView
                                        android:id="@+id/glassB"
                                        android:layout_width="50dp"
                                        android:layout_height="50dp"
                                        android:layout_gravity="center"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toTopOf="parent"
                                        app:lottie_autoPlay="false"
                                        app:lottie_loop="false"
                                        app:lottie_rawRes="@raw/glass_fill"
                                        tools:ignore="NotSibling" />

                                    <com.airbnb.lottie.LottieAnimationView
                                        android:id="@+id/glassBb"
                                        android:layout_width="50dp"
                                        android:layout_height="50dp"
                                        android:layout_gravity="center"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toTopOf="parent"
                                        app:lottie_autoPlay="false"
                                        app:lottie_loop="false"
                                        app:lottie_rawRes="@raw/glass_fill_reverse"
                                        tools:ignore="NotSibling" />
                                </androidx.constraintlayout.widget.ConstraintLayout>

                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:id="@+id/layout3"
                                    android:layout_margin="2dp"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content">


                                    <com.airbnb.lottie.LottieAnimationView
                                        android:id="@+id/glassC"
                                        android:layout_width="50dp"
                                        android:layout_height="50dp"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toTopOf="parent"
                                        app:lottie_autoPlay="false"
                                        app:lottie_loop="false"
                                        app:lottie_rawRes="@raw/glass_fill" />

                                    <com.airbnb.lottie.LottieAnimationView
                                        android:id="@+id/glassCc"
                                        android:layout_width="50dp"
                                        android:layout_height="50dp"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toTopOf="parent"
                                        app:lottie_autoPlay="false"
                                        app:lottie_loop="false"
                                        app:lottie_rawRes="@raw/glass_fill_reverse" />
                                </androidx.constraintlayout.widget.ConstraintLayout>

                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:id="@+id/layout4"
                                    android:layout_margin="2dp"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content">

                                    <com.airbnb.lottie.LottieAnimationView
                                        android:id="@+id/glassD"
                                        android:layout_width="50dp"
                                        android:layout_height="50dp"
                                        android:layout_gravity="center"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toTopOf="parent"
                                        app:lottie_autoPlay="false"
                                        app:lottie_loop="false"
                                        app:lottie_rawRes="@raw/glass_fill" />

                                    <com.airbnb.lottie.LottieAnimationView
                                        android:id="@+id/glassDd"
                                        android:layout_width="50dp"
                                        android:layout_height="50dp"
                                        android:layout_gravity="center"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toTopOf="parent"
                                        app:lottie_autoPlay="false"
                                        app:lottie_loop="false"
                                        app:lottie_rawRes="@raw/glass_fill_reverse" />
                                </androidx.constraintlayout.widget.ConstraintLayout>

                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:id="@+id/layout5"
                                    android:layout_margin="2dp"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content">


                                    <com.airbnb.lottie.LottieAnimationView
                                        android:id="@+id/glassE"
                                        android:layout_width="50dp"
                                        android:layout_height="50dp"
                                        android:layout_gravity="center"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toTopOf="parent"
                                        app:lottie_autoPlay="false"
                                        app:lottie_loop="false"
                                        app:lottie_rawRes="@raw/glass_fill" />

                                    <com.airbnb.lottie.LottieAnimationView
                                        android:id="@+id/glassEe"
                                        android:layout_width="50dp"
                                        android:layout_height="50dp"
                                        android:layout_gravity="center"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toTopOf="parent"
                                        app:lottie_autoPlay="false"
                                        app:lottie_loop="false"
                                        app:lottie_rawRes="@raw/glass_fill_reverse" />

                                </androidx.constraintlayout.widget.ConstraintLayout>

                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:id="@+id/layout6"
                                    android:layout_margin="2dp"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content">


                                    <com.airbnb.lottie.LottieAnimationView
                                        android:id="@+id/glassF"
                                        android:layout_width="50dp"
                                        android:layout_height="50dp"
                                        android:layout_gravity="center"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toTopOf="parent"
                                        app:lottie_autoPlay="false"
                                        app:lottie_loop="false"
                                        app:lottie_rawRes="@raw/glass_fill" />

                                    <com.airbnb.lottie.LottieAnimationView
                                        android:id="@+id/glassFf"
                                        android:layout_width="50dp"
                                        android:layout_height="50dp"
                                        android:layout_gravity="center"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toTopOf="parent"
                                        app:lottie_autoPlay="false"
                                        app:lottie_loop="false"
                                        app:lottie_rawRes="@raw/glass_fill_reverse" />
                                </androidx.constraintlayout.widget.ConstraintLayout>
                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:id="@+id/layout7"
                                    android:layout_margin="2dp"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content">


                                    <com.airbnb.lottie.LottieAnimationView
                                        android:id="@+id/glassG"
                                        android:layout_width="50dp"
                                        android:layout_height="50dp"
                                        android:layout_gravity="center"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toTopOf="parent"
                                        app:lottie_autoPlay="false"
                                        app:lottie_loop="false"
                                        app:lottie_rawRes="@raw/glass_fill" />

                                    <com.airbnb.lottie.LottieAnimationView
                                        android:id="@+id/glassGg"
                                        android:layout_width="50dp"
                                        android:layout_height="50dp"
                                        android:layout_gravity="center"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toTopOf="parent"
                                        app:lottie_autoPlay="false"
                                        app:lottie_loop="false"
                                        app:lottie_rawRes="@raw/glass_fill_reverse" />
                                </androidx.constraintlayout.widget.ConstraintLayout>
                            </LinearLayout>
                        </LinearLayout>
                    </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:id="@+id/card1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="20dp">

                            <androidx.constraintlayout.widget.ConstraintLayout
                                android:id="@+id/breakfast_section"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:padding="10dp">

                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:src="@drawable/ic_plus_svgrepo_com"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintEnd_toEndOf="parent"
                                    app:layout_constraintTop_toTopOf="parent" />

                                <com.airbnb.lottie.LottieAnimationView
                                    android:id="@+id/animationView"
                                    android:layout_width="40dp"
                                    android:layout_height="40dp"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintStart_toStartOf="parent"
                                    app:layout_constraintTop_toBottomOf="@id/app_bar"
                                    app:lottie_autoPlay="true"
                                    app:lottie_loop="true"
                                    app:lottie_rawRes="@raw/toast"
                                    tools:ignore="NotSibling" />

                                <TextView
                                    android:id="@+id/breakfast"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="10dp"
                                    android:text="Breakfast"
                                    android:textColor="@color/black"
                                    android:textSize="20sp"
                                    android:textStyle="bold"
                                    app:layout_constraintStart_toEndOf="@id/animationView"
                                    app:layout_constraintTop_toTopOf="parent" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="10dp"
                                    android:text="Recommended297-446 kcal"
                                    android:textColor="@color/black"
                                    app:layout_constraintStart_toEndOf="@id/animationView"
                                    app:layout_constraintTop_toBottomOf="@id/breakfast" />

                            </androidx.constraintlayout.widget.ConstraintLayout>
                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:id="@+id/card2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/card5"
                            android:layout_margin="20dp">

                            <androidx.constraintlayout.widget.ConstraintLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:padding="10dp">

                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:src="@drawable/ic_plus_svgrepo_com"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintEnd_toEndOf="parent"
                                    app:layout_constraintTop_toTopOf="parent" />

                                <com.airbnb.lottie.LottieAnimationView
                                    android:id="@+id/aniLaunch"
                                    android:layout_width="40dp"
                                    android:layout_height="40dp"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintStart_toStartOf="parent"
                                    app:layout_constraintTop_toBottomOf="@id/app_bar"
                                    app:lottie_autoPlay="true"
                                    app:lottie_loop="true"
                                    app:lottie_rawRes="@raw/foodprepared"
                                    tools:ignore="NotSibling" />

                                <TextView
                                    android:id="@+id/lunch"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="10dp"
                                    android:text="Add Dinner"
                                    android:textColor="@color/black"
                                    android:textSize="20sp"
                                    android:textStyle="bold"
                                    app:layout_constraintStart_toEndOf="@id/aniLaunch"
                                    app:layout_constraintTop_toTopOf="parent" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="10dp"
                                    android:text="Recommended466-594 kcal"
                                    android:textColor="@color/black"
                                    app:layout_constraintStart_toEndOf="@id/aniLaunch"
                                    app:layout_constraintTop_toBottomOf="@id/lunch" />

                            </androidx.constraintlayout.widget.ConstraintLayout>
                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:id="@+id/card3"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/card2"
                            android:layout_margin="20dp">

                            <androidx.constraintlayout.widget.ConstraintLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:padding="10dp">

                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:src="@drawable/ic_plus_svgrepo_com"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintEnd_toEndOf="parent"
                                    app:layout_constraintTop_toTopOf="parent" />

                                <com.airbnb.lottie.LottieAnimationView
                                    android:id="@+id/aniDinner"
                                    android:layout_width="40dp"
                                    android:layout_height="40dp"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintStart_toStartOf="parent"
                                    app:layout_constraintTop_toBottomOf="@id/app_bar"
                                    app:lottie_autoPlay="true"
                                    app:lottie_loop="true"
                                    app:lottie_rawRes="@raw/drink"
                                    tools:ignore="NotSibling" />

                                <TextView
                                    android:id="@+id/dinner"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="10dp"
                                    android:text="Add Snack"
                                    android:textColor="@color/black"
                                    android:textSize="20sp"
                                    android:textStyle="bold"
                                    app:layout_constraintStart_toEndOf="@id/aniDinner"
                                    app:layout_constraintTop_toTopOf="parent" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="10dp"
                                    android:text="Recommended74-149 kcal"
                                    android:textColor="@color/black"
                                    app:layout_constraintStart_toEndOf="@id/aniDinner"
                                    app:layout_constraintTop_toBottomOf="@id/dinner" />

                            </androidx.constraintlayout.widget.ConstraintLayout>
                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:id="@+id/card4"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/card3"
                            android:layout_margin="20dp">

                            <androidx.constraintlayout.widget.ConstraintLayout
                                android:layout_width="match_parent"
